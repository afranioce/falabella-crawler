{
  "type": "project",
  "description": "description",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "proprietary",
  "require": {
    "alefcastelo/crawler": "dev-master",
    "ext-json": "*",
    "linio/util": "dev-master",
    "linio/database": "dev-master",
    "symfony/dependency-injection": "^5.1",
    "symfony/dotenv": "^5.1",
    "symfony/framework-bundle": "^5.1",
    "symfony/yaml": "^5.1",
    "guzzlehttp/guzzle": "^7.1"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/alefcastelo/crawler.git"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.4@dev",
    "friendsofphp/php-cs-fixer": "^2.17@dev"
  },
  "scripts": {
    "lint": "php-cs-fixer fix --verbose --show-progress=estimating",
    "lint:check": [
      "php-cs-fixer fix --dry-run --verbose --show-progress=estimating"
    ],
    "phpunit": "phpunit",
    "test": [
      "@lint:check",
      "@phpunit"
    ],
    "serve": "APP_ENV=test php -d variables_order=EGPCS -S localhost:8080 -t public/"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "^5.1"
    }
  }
}
